class Array {
    public static void main () {
        System.out.println (new ArrayClass ().run ());
    }
}

class ArrayClass {

    public int run () {
        int[] arr;
        int v;
        v = 0;
        arr = new int[10];
        while (v < 10) {
            arr[v] = v;
            v = v + 1;
        }
        return arr[5];
    }

}
