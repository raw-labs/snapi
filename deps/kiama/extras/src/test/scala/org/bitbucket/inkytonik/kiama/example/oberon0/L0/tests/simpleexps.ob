    bvar1 := bvar1 OR (bvar2 & FALSE);
    bvar1 := bvar1 & (TRUE & bvar2);
    bvar1 := ~TRUE;
    bvar1 := ~bvar1 & bvar2;
    bvar1 := ~~bvar1;
    
    bvar1 := ivar1 = ivar2;
    bvar1 := ivar1 # ivar2;
    bvar1 := ivar1 < ivar2;
    bvar1 := ivar1 <= ivar2;
    bvar1 := ivar1 > ivar2;
    bvar1 := ivar1 >= ivar2;
    
    bvar1 := (ivar1 = ivar2) OR bvar2;
    bvar1 := bvar2 & (ivar1 # ivar2)
END simpleexps.
