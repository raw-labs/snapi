MODULE simpleexps;

VAR
    ivar1, ivar2 : INTEGER;
    bvar1, bvar2 : BOOLEAN;

BEGIN
    ivar1 := 0;
    ivar1 := 12; 
    ivar1 := 345; 
    ivar1 := 67890123;
    
    ivar1 := -0;
    ivar1 := -1;
    ivar2 := -782374;
    ivar2 := --10;

    ivar1 := ivar2 + 1;
    ivar1 := 2 + ivar2;
    ivar1 := ivar2 - ivar1;
    ivar1 := +3 - ivar2;
    ivar1 := ivar2 * 4;
    ivar1 := 5 * ivar2;
    ivar1 := ivar2 DIV 6;
    ivar1 := -7 DIV (ivar2 + 1);
    ivar1 := ivar2 MOD ivar1;
    ivar1 := 8 MOD (ivar2 + 1);
    
    ivar1 := 2 + 3 * 4;
    ivar1 := (2 + 3) * 4;
    ivar1 := 2 - 3 DIV 4;
    ivar1 := (2 - 3) DIV 4;
    ivar1 := 2 + 3 MOD 4;
    ivar1 := (2 + 3) MOD 4;
    ivar1 := 2 + 3 + 4;
    ivar1 := 2 + (3 + 4);
